{
  "name": "rhfa-material-ui",
  "version": "1.2.4",
  "description": "Generate automatic forms following a schema using Material-UI",
  "main": "lib/index.js",
  "keywords": [
    "react",
    "reactjs",
    "hooks",
    "react-hook-form",
    "form",
    "validators",
    "validation",
    "material-ui"
  ],
  "author": "David Gonz√°lez <braben@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dgonz64/rhfa-material-ui"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "babel-loader": "^9.1.3",
    "enzyme": "^3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "jest": "^27.5.1",
    "jsdoc-to-markdown": "^7.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.51.5",
    "rimraf": "^5.0.7",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf lib",
    "start": "npm run clean && babel src --out-dir lib --watch --verbose --source-maps",
    "build": "npm run clean && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "babel src --out-dir lib",
    "build:umd": "webpack",
    "build:umd:min": "NODE_ENV=production npm run build:umd",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:short": "jest",
    "test": "jest --verbose",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "react-hook-form-auto": "^1.3.15"
  }
}
